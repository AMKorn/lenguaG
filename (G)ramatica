BODY -> DECLARATIONS MAIN.
MAIN -> main (ARGS) L_KEY INSTRUCTIONS R_KEY.
DECLARATIONS -> DECLARATION DECLARATIONS 
    | FUNCTION DECLARATIONS 
    | .

DECLARATION -> TYPE IDENTIFIER INITIALIZATION ENDLINE.
INITIALIZATION -> EQUAL VALUE.

FUNCTION -> TYPE IDENTIFIER (ARGS) L_KEY INSTRUCTIONS R_KEY.

ARGS -> ARG MORE_ARGS
    | .

MORE_ARGS -> COMMA ARG
    | .
